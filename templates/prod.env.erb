MINIO_ACCESS_KEY=<%= @role_salep::miniokey %>
MINIO_SECRET_KEY=<%= @role_salep::miniosecret %>
# Use TRAEFIK_TIML_FILE without quotes!
TRAEFIK_TOML_FILE=<%= @role_salep::traefik_toml_file %>
ACME_JSON=<%= @role_salep::traefik_acme_json %>
# username / password can be created with htpasswd -n <username>
# multiple un/pw can be separated with ,
# $ HAS to BE ESACPED with $ (so change $ with $$)
# default is kibana / kibana
KIBANA_AUTH="<%= @role_salep::kibana_auth %>"
#URL configs for salep
MINIO_URL_CONFIG="traefik.frontend.rule=Host:<%= @role_salep::minio_url %>"
KIBANA_URL_CONFIG="traefik.frontend.rule=Host:<%= @role_salep::kibana_url %>"
